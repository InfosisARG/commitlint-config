{
	"baseUrl": "https://github.com/InfosisARG/commitlint-config",
	"ignorePatterns": [
		{
			"pattern": "^https://infosisglobal.com"
		},
		{
			"pattern": "^https://github.com/InfosisARG/commitlint-config"
		},
		{
			"pattern": "^https://github.com/infosisarg/commitlint-config/compare.*$"
		},
		{
			"pattern": "^https://github.com/InfosisARG/commitlint-config/releases.*$"
		},
		{
			"pattern": "^https://github.com/InfosisARG/commitlint-config/pulls.*$"
		},
		{
			"pattern": "^https://github.com/InfosisARG/commitlint-config/actions.*$"
		},
		{
			"pattern": "^https://github.com/InfosisARG/commitlint-config/issues.*$"
		}
	],
	"replacementPatterns": [
		{
			"pattern": "^/docs",
			"replacement": "{{BASEURL}}/docs"
		}
	],
	"timeout": "20s",
	"retryCount": 3,
	"fallbackRetryDelay": "30s",
	"aliveStatusCodes": [200, 206]
}
